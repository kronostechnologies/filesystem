{
  "name": "kronos/filesystem",
  "type": "library",
  "description": "Kronos Filesystem abstraction library",
  "homepage": "https://github.com/kronostechnologies/filesystem",
  "license": "MIT",
  "keywords": [
    "kronos",
    "file system",
    "flysystem",
    "s3"
  ],
  "config": {
    "platform": {
      "php": "7.4"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "composer/package-versions-deprecated": true,
      "cyclonedx/cyclonedx-php-composer": true
    }
  },
  "autoload": {
    "psr-4": {
      "Kronos\\FileSystem\\": "Kronos/FileSystem"
    }
  },
  "require": {
    "php": ">=5.6",
    "league/flysystem": "1.0.* || 1.1.*",
    "league/flysystem-aws-s3-v3": "1.0.*",
    "elao/enum": "^1.12",
    "guzzlehttp/guzzle": "^6.5.5",
    "guzzlehttp/promises": "^1.5.1",
    "guzzlehttp/psr7": "^1.8.5"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.20",
    "dealerdirect/phpcodesniffer-composer-installer": "0.7.2",
    "phpcompatibility/php-compatibility": "9.3.5",
    "vimeo/psalm": "4.23.0",
    "cyclonedx/cyclonedx-php-composer": "3.10.0",
    "psalm/plugin-phpunit": "0.16.1"
  }
}
